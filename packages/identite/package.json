{
  "name": "@gouvfr-lasuite/proconnect.identite",
  "version": "0.9.0",
  "homepage": "https://github.com/proconnect-gouv/proconnect-identite/tree/master/packages/identite#readme",
  "bugs": "https://github.com/proconnect-gouv/proconnect-identite/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proconnect-gouv/proconnect-identite.git",
    "directory": "packages/identite"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#src/*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    },
    "#testing": "./testing/index.ts",
    "#testing/*": "./testing/*/index.ts"
  },
  "exports": {
    "./*": {
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.lib.json",
    "check": "npm run build -- --noEmit",
    "clean": "rm -rf dist tsconfig*.tsbuildinfo",
    "dev": "npm run build -- --watch --preserveWatchOutput",
    "dev:test": "tsx --watch --test src/**/*.test.ts",
    "test": "tsx --test --test-reporter=spec src/**/*.test.ts",
    "test:update-snapshots": "tsx --test --test-reporter=spec --test-update-snapshots src/**/*.test.ts"
  },
  "dependencies": {
    "leven": "^4.0.0",
    "openid-client": "^6.6.4",
    "sql-template-tag": "^5.2.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.4",
    "@gouvfr-lasuite/proconnect.core": "^0.6.0",
    "@gouvfr-lasuite/proconnect.devtools.typescript": "0.0.0",
    "@gouvfr-lasuite/proconnect.entreprise": "^0.0.0",
    "@gouvfr-lasuite/proconnect.insee": "^0.0.0",
    "@sinonjs/fake-timers": "^14.0.0",
    "@types/node": "^24.3.1",
    "await-to-js": "^3.0.0",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.16.3",
    "tsx": "^4.20.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
