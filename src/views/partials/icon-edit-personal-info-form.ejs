<form
  class="inline-form"
  autocomplete="off"
  action="<%= actionForUpdateModeration; %>"
  method="post"
>
  <input type="hidden" name="_csrf" value="<%= csrfToken; %>" />
  <button
    type="submit"
    class="fr-icon-edit-fill blue-france<%= locals.confirmMessage ? ' js-confirm' : '' %>"
    data-confirm="<%= locals.confirmMessage; %>"
    aria-label="<%=messageToDisplay; %>"
    title="<%=messageToDisplay; %>"
  ></button>
</form>
